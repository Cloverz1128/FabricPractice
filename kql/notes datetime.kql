| distinct StartTime, EndTime
| extend Calc = format_datetime(StartTime, 'd MM yyyy'),
         Calc2 = format_timespan(EndTime - StartTime, 'ddd.hh:mm:ss')
| where StartTime between (datetime(2007-03-26) .. datetime(2007-04-15))


